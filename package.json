        "@radix-ui/react-popover": "^0.1.6",
        "@radix-ui/react-slider": "0.1.3",
        "@radix-ui/react-tabs": "0.1.4",
        "ethers": "5.5.4",
        "framer-motion": "^6.3.11",
        "hotkeys-js": "^3.9.4",
        "next": "12.0.8",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hot-toast": "2.2.0",
        "react-hotkeys-hook": "^3.4.6",
        "react-popper": "^2.2.5",
        "recharts": "2.1.8",
        "swr": "1.1.2",
        "use-persisted-state": "0.3.3",
